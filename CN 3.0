1.4 TCP PRIME NUMBER SERVER & CLIENT
1.4a tcp_prime_server.py
import socket

def check_prime(n):
    if n < 2: return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0: return False
    return True

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.bind(('', 9999))
s.listen(5)
print("TCP Prime Server Running...")
while True:
    c, addr = s.accept()
    n = int(c.recv(1024).decode())
    is_prime = check_prime(n)
    result = "Prime" if is_prime else "Not Prime"
    c.send(result.encode())
    c.close()
