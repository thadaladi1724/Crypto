3.5 XML-RPC ALL-IN-ONE CALCULATOR SERVER & CLIENT (Add + Fib + Prime + Sort)
3.5a xml_calc_server.py
from xmlrpc.server import SimpleXMLRPCServer

def add_numbers(x, y):
    return x + y

def compute_fib(n):
    if n <= 0: return 0
    a, b = 0, 1
    for _ in range(n):
        a, b = b, a + b
    return a

def check_prime(n):
    if n < 2: return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0: return False
    return True

def sort_numbers(nums_list):
    return sorted(nums_list)

server = SimpleXMLRPCServer(('', 9999))
server.register_function(add_numbers, "add")
server.register_function(compute_fib, "fib")
server.register_function(check_prime, "prime")
server.register_function(sort_numbers, "sort")
print("XML-RPC Calculator Server Running...")
server.serve_forever()
3.5b xml_calc_client.py
import xmlrpc.client

proxy = xmlrpc.client.ServerProxy("http://YOUR_IP_HERE:9999")
print("Add 10+20 =", proxy.add(10, 20))
print("Fib(10) =", proxy.fib(10))
print("Prime 17?", proxy.prime(17))
print("Sort [5,2,9] =", proxy.sort([5, 2, 9]))
