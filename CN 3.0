1.3 TCP FIBONACCI SERVER & CLIENT
1.3a tcp_fib_server.py
import socket

def compute_fib(n):
    if n <= 0: return 0
    a, b = 0, 1
    for _ in range(n):
        a, b = b, a + b
    return a

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.bind(('', 9999))
s.listen(5)
print("TCP Fibonacci Server Ready...")
while True:
    c, addr = s.accept()
    n = int(c.recv(1024).decode())
    fib_result = compute_fib(n)
    c.send(str(fib_result).encode())
    c.close()
