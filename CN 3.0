1.7 TCP FILE TRANSFER SERVER & CLIENT
1.7a tcp_file_server.py
import socket

def save_file(filename, data):
    with open("received_" + filename, "wb") as f:
        f.write(data)
    return True

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.bind(('', 9999))
s.listen(1)
print("File Server Waiting...")
c, addr = s.accept()
print("Connected:", addr)
filename = c.recv(1024).decode()
file_data = c.recv(1024*1024)
save_file(filename, file_data)
print("File Received:", filename)
c.close()
1.7b tcp_file_client.py
import socket
import os

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.connect(('YOUR_IP_HERE', 9999))
file_path = input("Enter filename to send: ")
filename = os.path.basename(file_path)
s.send(filename.encode())
with open(file_path, "rb") as f:
    s.send(f.read())
print("File Sent!")
s.close()
