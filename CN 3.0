1.1 TCP ECHO SERVER & CLIENT
1.1a tcp_echo_server.py
import socket

def echo_message(data):
    return data

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.bind(('', 9999))
s.listen(5)
print("TCP Echo Server Running on port 9999...")
while True:
    c, addr = s.accept()
    print("Connected:", addr)
    while True:
        data = c.recv(1024).decode()
        if not data or data.lower() == "exit": break
        response = echo_message(data)
        c.send(response.encode())
    c.close()
1.1b tcp_echo_client.py
import socket

s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.connect(('YOUR_IP_HERE', 9999))
print("Connected! Type 'exit' to quit")
while True:
    msg = input("You → ")
    s.send(msg.encode())
    if msg.lower() == "exit": break
    print("Server →", s.recv(1024).decode())
s.close()
